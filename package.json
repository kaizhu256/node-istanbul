{
  "author": "kai zhu <kaizhu256@gmail.com>",
  "bin": { "istanbul-lite" : "index.js" },
  "description": "lightweight browser/nodejs version of istanbul coverage with zero dependencies",
  "devDependencies": {
    "utility2": "2015.2.21-10"
  },
  "engines": { "node": "^0.10.32" },
  "keywords": [
    "browser",
    "code",
    "cover",
    "coverage",
    "istanbul",
    "instrument",
    "lightweight",
    "utility2"
  ],
  "license": "MIT",
  "name": "istanbul-lite",
  "os": ["darwin", "linux"],
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/kaizhu256/node-istanbul-lite.git"
  },
  "scripts": {
    "build2": "node_modules/.bin/utility2 shRun shBuild",
    "start": "npm_config_mode_auto_restart=1 node_modules/.bin/utility2 shRun node test.js",
    "test": "node_modules/.bin/utility2 shRun shNpmTest test.js"
  },
  "version": "2015.2.21-10"
}
